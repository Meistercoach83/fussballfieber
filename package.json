{
  "author": "Thomas Handle <thomas.handle@mail.com>",
  "name": "fussballfieber2019.de",
  "description": "Homepage",
  "homepage": "https://fussballfieber2019.de",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meistercoach83/fussballfieber.git"
  },
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "main": "src/main.ts",
  "scripts": {
    "analyzer": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "ng": "ng",
    "changelog": "standard-changelog",
    "start": "ng serve",
    "start:srr": "npm run serve:ssr",
    "build": "ng build",
    "build:ci": "npm run build:prod && npm run build:prod:aot",
    "build:prod": "npm run build --prod",
    "build:prod:aot": "npm run build -- --aot",
    "test": "ng test",
    "test:coverage": "npm run clean:coverage && ng test -sr -cc",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "postinstall": "npm run tsformat",
    "tsformat": "tsfmt -r",
    "pwa": "ng build --prod && sw-precache --root=dist --config=precache-config.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/app/server/server.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run application:server:production"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@agm/snazzy-info-window": "^1.0.0-beta.5",
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.2.1",
    "@angular/cli": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/platform-server": "^7.2.1",
    "@angular/pwa": "^0.12.2",
    "@angular/router": "^7.2.1",
    "@angular/service-worker": "^7.2.1",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-loading-bar/router": "^4.1.1",
    "@ngx-meta/core": "^7.0.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^2.0.0",
    "@trademe/ng-defer-load": "^2.0.0",
    "@types/node": "^10.12.1",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ngx-cookie": "^4.1.2",
    "ngx-countdown-timer": "^0.1.9",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-pipes": "^2.3.9",
    "node-sass": "^4.11.0",
    "popper": "^1.0.1",
    "rxjs": "^6.3.3",
    "snazzy-info-window": "^1.1.1",
    "ts-loader": "^5.3.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.2.1",
    "@types/jasmine": "^3.3.8",
    "@types/jquery": "^3.3.22",
    "bufferutil": "^4.0.1",
    "codelyzer": "^4.4.4",
    "domino": "^2.1.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4",
    "typescript-formatter": "^7.2.1",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.2.1",
    "ws": "^6.1.3",
    "xhr2": "^0.1.4"
  }
}
